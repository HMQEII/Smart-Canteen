{
    "functions": {
      "backend/wsgi.py": {
        "runtime": "python3.9",
        "maxLambdaSize": "250mb",
        "externalPackages": [
          "asgiref",
          "attrs",
          "blinker",
          "certifi",
          "cffi",
          "charset-normalizer",
          "click",
          "colorama",
          "contourpy",
          "cycler",
          "Django",
          "Flask",
          "fonttools",
          "h11",
          "idna",
          "itsdangerous",
          "Jinja2",
          "joblib",
          "kiwisolver",
          "MarkupSafe",
          "matplotlib",
          "mrjob",
          "nltk",
          "numpy",
          "opencv-python-headless",
          "outcome",
          "packaging",
          "pandas",
          "pillow",
          "pycparser",
          "pyparsing",
          "PySocks",
          "python-dateutil",
          "pytz",
          "PyYAML",
          "pyzbar",
          "razorpay",
          "regex",
          "requests",
          "scikit-learn",
          "scipy",
          "selenium",
          "setuptools",
          "six",
          "sniffio",
          "sortedcontainers",
          "sqlparse",
          "threadpoolctl",
          "tqdm",
          "trio",
          "trio-websocket",
          "typing_extensions",
          "tzdata",
          "urllib3",
          "Werkzeug",
          "wsproto"
        ]
      }
    },
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "backend/wsgi.py"
      }
    ]
  }
  